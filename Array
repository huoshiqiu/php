<?php

$stack=array("orange","banana");
echo "原来的数组";
echo "<br>";
print_r($stack);
echo "<br>";
array_push($stack, "apple","jkshgd");
echo "入栈后的数组";
echo "<br>";
print_r($stack);
$fruit=array_pop($stack);
sort($stack);
print_r($stack);


$country=array('C'=>"China",'K'=>"Koera",'A'=>"America");
echo "<br />&nbsp;<br />";
foreach ($country as $key=>$value)
{
  echo "$key 代表：$value";
	echo "<br />&nbsp<br />";
}

echo $_SERVER['HTTP_USER_AGENT'];
echo "<br>";
echo $_SERVER['HTTP_ACCEPT_LANGUAGE'];




?>
